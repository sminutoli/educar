package com.educar.sopa2 {	import flash.display.Sprite;	import com.educar.sopa.BtnAnswer;	import com.educar.sopa.GridSolution;		public class NavAnswer extends Sprite {		public function NavAnswer() {			var nY:uint = 0;			var nSpaceY:uint = 3;						var solutions:Array = GridSolutionFactory.getSolutions();			var s:GridSolution;						for( var i:uint=0; s = solutions[i]; i++ ){								var btn:BtnAnswer = new BtnAnswer( s.id, s.label );				this.addChild( btn );				btn.y = nY;				nY += btn.height + nSpaceY;								btn.show( 1000 + (i)*100 );							}		}	}	}