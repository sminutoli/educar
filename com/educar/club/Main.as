package com.educar.club {		import flash.display.MovieClip;	import com.educar.game.Game;	import flash.events.Event;	import com.educar.game.GameEvent;	import flash.display.Sprite;			public class Main extends Game {				//FLA vars		public var mcFrame:Sprite;				//Class vars		private static var instance:Main;						public static function getInstance():Main {			return Main.instance;		}				public function Main(){			super();			if( Main.instance ) throw new Error('Main is a singleton');			Main.instance = this;					}				//remove behaviour inherited		override protected function createBlocking():void {}		override protected function showPanel(e:Event):void {}		override protected function hidePanel(e:Event):void {}		override protected function bindEvents():void {			super.bindEvents();			this.stage.addEventListener( GameEvent.GAME_WIN, showEnd, false, 0, true );		}				private function showEnd(e:Event):void {			var panel:WinPanel = new WinPanel();			stage.addChild( panel );		}				override protected function showRules(e:Event):void {			var panel:RulesPanel = new RulesPanel();			stage.addChild( panel );		}				override protected function createButtons(e:GameEvent=null):void {			trace('Main createButtons');			var panel:Sprite = new QuestionsPanel();			this.addChildAt( panel, this.getChildIndex( this.mcFrame )+1 );		}			}	}