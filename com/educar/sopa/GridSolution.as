package com.educar.sopa {	import flash.geom.Point;		public class GridSolution {				private var _start:Point;		private var _end:Point;		private var _id:uint;		private var _label:String;				public function GridSolution( id:uint, label:String, start:Point, end:Point ) {			// constructor code			this._id = id;			this._start = start;			this._end = end;			this._label = label;					}				public function get id():uint { return this._id; }		public function get label():String { return this._label; }		public function get start():Point { return this._start; }		public function get end():Point { return this._end; }				public function check( pos1:Point, pos2:Point ):Boolean {			return ( this._start.equals( pos1 ) && this._end.equals( pos2 ) )				|| 				( this._start.equals( pos2 ) && this._end.equals( pos1 ) );		}	}	}