package com.educar.sopa {	import flash.events.Event;		public class GridEvent extends Event {				public static const SHOW_HINT:String = 'grid_show_hint';		public static const HIDE_HINT:String = 'grid_hide_hint';		public static const SOLUTION_OK:String = 'grid_solution_ok';		public static const SOLUTION_WRONG:String = 'grid_solution_wrong';		public static const GRID_COMPLETE:String = 'grid_complete';		public static const SHOW_SOLUTION:String = 'grid_show_solution';		public static const SHOW_SOLUTION_END:String = 'grid_show_solution_end';				private var _solution:GridSolution;				public function GridEvent( type:String, solution:GridSolution=null, bubbles:Boolean=true, cancelable:Boolean=false ) {			// constructor code			super( type, bubbles, cancelable );			this._solution = solution;		}				public override function clone():Event {			return new GridEvent( type, _solution, bubbles, cancelable );		}				public function get solution():GridSolution {			return this._solution;		}	}	}