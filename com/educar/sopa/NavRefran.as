package com.educar.sopa {	import flash.display.Sprite;		public class NavRefran extends Sprite {		public function NavRefran() {						var nY:uint = 0;			var nSpaceY:uint = 0;						var solutions:Array = GridSolutionFactory.getSolutions();			var s:GridSolution;						for( var i:uint=0; s = solutions[i]; i++ ){								var btn:BtnRefran = new BtnRefran( s.id, s.id.toString() );				this.addChild( btn );				btn.y = nY;				nY += btn.height + nSpaceY;								btn.show( 1500 + (i)*100 );							}					}	}	}