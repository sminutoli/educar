package com.educar.sopa {	import flash.display.Sprite;	import com.greensock.TweenLite;	import flash.events.Event;	import flash.events.MouseEvent;	import com.educar.game.GameEvent;		public class BtnReplay extends SopaBtn {				private var enabled:Boolean;				public function BtnReplay() {			// constructor code			super();			this.show();			this.setEnabled( false );		}		override protected function init( e:Event ):void {			super.init( e );			this.stage.addEventListener( GridEvent.GRID_COMPLETE, gridComplete, false, 0, true );		}				private function gridComplete(e:GridEvent):void {			this.setEnabled( true );		}		override protected function btn_click( e:MouseEvent ):void {			this.dispatchEvent( new GameEvent( GameEvent.GAME_REPLAY, true ) );		}	}	}