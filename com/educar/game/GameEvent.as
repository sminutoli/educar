package com.educar.game {	import flash.events.Event;		public class GameEvent extends Event {				public static const GAME_SHOW_RULES:String = 'educar_game_show_rules';		public static const GAME_START:String = 'educar_game_start';		public static const GAME_WIN:String = 'educar_game_win';		public static const GAME_REPLAY:String = 'educar_game_replay';				public function GameEvent( type:String, bubbles:Boolean=true, cancelable:Boolean=false ) {			// constructor code			super( type, bubbles, cancelable );		}				public override function clone():Event {			return new GameEvent( type, bubbles, cancelable );		}	}	}